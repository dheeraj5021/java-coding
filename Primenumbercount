import java.util.*;
class Primecount
{
 public static void main(String args[])
  {
    Scanner sc=new Scanner(System.in);
    int n,i,j;
    n=sc.nextInt();
    int a[]=new int[n];
    int count=0;
     for(i=0;i<n;i++)
     {
       a[i]=sc.nextInt();
      }
      for(i=0;i<n;i++)
    {
       int temp=0;
       for(j=2;j<a[i];j++)
{
      if(a[i]%j==0)
{
      temp++;
 }}   
     if(temp==0)
     count++;
   }
     System.out.println("count is"+count);
  
}}
