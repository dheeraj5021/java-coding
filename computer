class Complete{


    // Function for finding maximum and value pair
    public static int multiply (int arr[], int n) 
    {
        int s=0,j=0;
        for(int i=0;i<n/2;i++)
        {
            s=s+arr[i];

        }
        for(int i=0;i<n/2;i++)
        {
            j=s+arr[i];
        }
   return s*j;
    }


}
