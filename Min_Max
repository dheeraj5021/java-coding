import java.util.*;
class Min_Max
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
int n=sc.nextInt();
int a[]=new int[n];
for(int i=0;i<n;i++)
a[i]=sc.nextInt();
int min=a[0],max=a[0];
for(int i=1;i<n;i++)
{
  min=(int)Math.min(a[i],min);
  max=(int)Math.max(a[i],max);
}
 System.out.println("min="+min +" "+"max= "+max);
}
}
