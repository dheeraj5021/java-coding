import java.util.*;
class Minimum 
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
int size,i;
System.out.println("Enter array size");
size=sc.nextInt();
int a[]=new int[size];
System.out.print("Enter elements");
for(i=0;i<size;i++)
{
a[i]=sc.nextInt();
}
Arrays.sort(a);
System.out.print("Min element is" +a[0]);
}
}
